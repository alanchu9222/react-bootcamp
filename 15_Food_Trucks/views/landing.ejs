      <% } %> <% if(success && success.length > 0){ %>
      <div class="alert alert-success" role="alert">
        <%= success %>
      </div>
      <% } %>
    </div>

    <div id="landing-header">
      <!-- <h1>Welcome to Melbourne Food Trucks!</h1> -->
      <div id="landing_text">
        <div id="flip">
          <div><div>Melbourne City Food Trucks</div></div>
          <div><div>Melbourne City Food Trucks</div></div>
          <div><div>Melbourne City Food Trucks</div></div>
        </div>
      </div>

      <a
        href="/foodtrucks"
        class="btn btn-lg btn-success btn-primary btn-ripple"
        >Start!</a
      >
    </div>

